from sys import argv
from theSimulator import theSimulator

def main(cmdLineArgs):
	if (len(cmdLineArgs) != 4):
		print('Command-line input must be delivered in the following format:')
		print('python simulator inst.txt data.txt output.txt')
	else:
		# print("STEP 1")
		mySimulation = theSimulator(cmdLineArgs[1],cmdLineArgs[2])

if __name__== '__main__':
	main(argv)